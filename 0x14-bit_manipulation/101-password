#include <stdio.h>
#include <stdlib.h>
#include "main.h"

/**
* generatePassword - A function that generates password
*
* Return: Password.
*/

char* generatePassword() {
    char* password = "Congratulations!";
    return password;
}

/**
* main - void
* followed by a new line.
*
* Return: Always 0
*/

int main() {
    char* password = generatePassword();

    printf("Generated Password: %s\n", password);

    return 0;
}

